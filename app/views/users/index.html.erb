<div id="intro">
  <h1>All users</h1>
</div>

<%= will_paginate %>

<div class="user-profile">
  <div class="profile row">
    <div class="col-md-4 avatar">
      <div class="wrapper">
          <ul class="all-users">
            <% @users.each do |user| %>
            <li>
              <%= render user %>
            </li>
            <% end %>
          </ul>
      </div>
    </div>
  </div>
</div>
